<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#00aed0" />
      <stroke
          android:width="1dp"
          android:color="#00aed0" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="1dp"
          android:right="10dp"
          android:bottom="1dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#00aed0"
          android:endColor="#00aed0"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#00aed0" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="1dp"
          android:right="10dp"
          android:bottom="1dp" />
    </shape>
  </item>
</selector>